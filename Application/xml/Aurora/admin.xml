    <task type="payroll/processPayroll,payroll/savePayroll">
        <!--
        array empInfo
        Get Employee Information
        Calculate Employee Join Day Month Year
        Render header process HTML
        -->
        <observer controller="Markaxis/Employee/EmployeeControl"/>
        <observer controller="Markaxis/Payroll/PayrollControl"/>
        <!--
        array payCal
        Get Pay Calendar
         -->
        <observer controller="Markaxis/Payroll/CalendarControl"/>
        <!--
        array office
        Get Work Days of Month
        Calculate Employee days if new joiner within the month
        -->
        <observer controller="Markaxis/Company/OfficeControl"/>
        <!--
        array items
        Get Basic Salary
        Get Ordinary Wages
        Get Deduction
        Get DeductionAW
        Get Additional
        -->
        <observer controller="Markaxis/Payroll/ItemControl"/>
        <observer controller="Markaxis/Payroll/UserControl"/>
        <!--
        array claims
        Get employee claims - return array list with remark and amount
        -->
        <observer controller="Markaxis/Expense/ClaimControl"/>
        <observer controller="Markaxis/Payroll/UserExpenseControl"/>
        <observer controller="Markaxis/Leave/LeaveApplyControl"/>
        <observer controller="Markaxis/Payroll/UserItemControl"/>
        <!--
        array competency
        Get employee competency
        -->
        <observer controller="Markaxis/Employee/CompetencyControl"/>
        <!--
        array taxGroups
        Get employee applied tax groups
        -->
        <observer controller="Markaxis/Payroll/TaxGroupControl"/>
        <!--
        array taxRules
        Get employee tax rules belong to groups
        -->
        <observer controller="Markaxis/Payroll/TaxRuleControl"/>
        <!--
        unset from taxRules if competency does not applies
        -->
        <observer controller="Markaxis/Payroll/TaxCompetencyControl"/>
        <!--
        unset from taxRules if race does not applies
        -->
        <observer controller="Markaxis/Payroll/TaxRaceControl"/>
        <!--
        unset from taxRules if age does not applies
        unset from taxRules if ordinary wage does not equates
        unset from taxRules if employee confirmation date not reached
        unset from taxRules if all pay items does not equates
        -->
        <observer controller="Markaxis/Payroll/TaxComputingControl"/>


        <observer controller="Markaxis/Payroll/TaxPayItemControl"/>

        <!--
        Render process header leave summary
        -->
        <observer controller="Markaxis/Payroll/LeaveControl"/>
        <!--
        Render taxgroup summary ruleTitle
        Apply taxrules to totalOrdinary and set itemRow
        Set contribution, skillLevy and foreign levy
        -->
        <observer controller="Markaxis/Payroll/TaxRuleWrapperControl"/>
        <!--
        Render process form and summary
        -->
        <observer controller="Markaxis/Payroll/UserTaxControl"/>
        <observer controller="Markaxis/Payroll/LevyControl"/>
        <observer controller="Markaxis/Payroll/ContributionControl"/>
        <observer controller="Markaxis/Payroll/SummaryControl"/>
    </task>

